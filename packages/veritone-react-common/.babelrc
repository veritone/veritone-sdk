{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-external-helpers",
    [
      "module-resolver",
      {
        "alias": {
          "helpers": "./src/helpers",
          "images": "./src/resources/images",
          "components": "./src/components",
          "reduxModules": "./src/redux/modules"
        }
      }
    ],
    "@babel/plugin-transform-react-display-name",
    "@babel/plugin-proposal-export-default-from",
    "@babel/plugin-proposal-export-namespace-from",
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-transform-runtime",
    "lodash",
    "date-fns"
  ],
  "env": {
    "test": {
      "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
      ],
      "plugins": [
        [
          "module-resolver",
          {
            "root": "./src",
            "alias": {
              "helpers": "./src/helpers",
              "images": "./src/resources/images",
              "components": "./src/components",
              "reduxModules": "./src/redux/modules"
            }
          }
        ],
        "@babel/plugin-proposal-export-default-from",
        "@babel/plugin-proposal-export-namespace-from",
        [
          "@babel/plugin-proposal-decorators",
          {
            "legacy": true
          }
        ],
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-proposal-object-rest-spread",
        "lodash"
      ]
    },
    "production-es": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false,
            "targets": {
              "browsers": [
                "> 5%"
              ]
            }
          }
        ],
        "@babel/preset-react",
      ],
      "plugins": [
        "@babel/plugin-external-helpers",
        [
          "module-resolver",
          {
            "alias": {
              "helpers": "./src/helpers",
              "images": "./src/resources/images",
              "components": "./src/components",
              "reduxModules": "./src/redux/modules"
            }
          }
        ],
        "@babel/plugin-transform-react-display-name",
        "@babel/plugin-proposal-export-default-from",
        "@babel/plugin-proposal-export-namespace-from",
        [
          "@babel/plugin-proposal-decorators",
          {
            "legacy": true
          }
        ],
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-proposal-object-rest-spread",
        "@babel/plugin-transform-runtime",
        "lodash",
        "date-fns"
      ]
    },
    "production-cjs": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false,
            "targets": {
              "browsers": [
                "> 0.5%"
              ]
            }
          }
        ],
        "@babel/preset-react"
      ],
      "plugins": [
        "@babel/plugin-external-helpers",
        [
          "module-resolver",
          {
            "alias": {
              "helpers": "./src/helpers",
              "images": "./src/resources/images",
              "components": "./src/components",
              "reduxModules": "./src/redux/modules"
            }
          }
        ],
        "@babel/plugin-transform-react-display-name",
        "@babel/plugin-proposal-export-default-from",
        "@babel/plugin-proposal-export-namespace-from",
        [
          "@babel/plugin-proposal-decorators",
          {
            "legacy": true
          }
        ],
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-proposal-object-rest-spread",
        "@babel/plugin-transform-runtime",
        "lodash",
        "date-fns"
      ]
    }
  }
}