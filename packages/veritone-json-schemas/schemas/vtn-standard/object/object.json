{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://docs.veritone.com/schemas/vtn-standard/object.json",
  "title": "vtn-standard.object",
  "description": "Standard engine output for Object at Veritone",
  "allOf": [
    {
      "$ref": "master.json#/definitions/header"
    },
    {
      "type": "object",
      "properties": {
        "validationContracts": {
          "type": "array",
          "contains": {
            "const": "object"
          }
        },
        "object": {
          "type": "array",
          "items": {
            "$ref": "master.json#/definitions/objectResult"
          }
        },
        "series": {
          "type": "array",
          "items": {
            "allOf": [
              {
                "$ref": "master.json#/definitions/seriesItem"
              },
              {
                "type": "object",
                "properties": {
                  "object": {
                    "$ref": "master.json#/definitions/noDocumentIndexing"
                  }
                },
                "required": [
                  "object"
                ]
              }
            ]
          }
        }
      }
    }
  ]
}
