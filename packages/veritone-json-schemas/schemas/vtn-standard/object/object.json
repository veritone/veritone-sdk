{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://docs.veritone.com/schemas/vtn-standard/object.json",
  "title": "vtn-standard.object",
  "description": "Standard engine output for Object at Veritone",
  "allOf": [
    {
      "$ref": "master.json#/definitions/header"
    },
    {
      "type": "object",
      "properties": {
        "validationContracts": {
          "type": "array",
          "contains": {
            "const": "object"
          }
        },
        "object": {
          "type": "array",
          "items": {
            "$ref": "master.json#/definitions/objectResult"
          }
        },
        "series": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "object": {
                "allOf": [
                  {
                    "$ref": "master.json#/definitions/objectResult"
                  },
                  {
                    "$ref": "master.json#/definitions/noDocumentIndexing"
                  }
                ]
              },
              "startTimeMs": {
                "type": "integer",
                "minimum": 0
              },
              "stopTimeMs": {
                "type": "integer",
                "minimum": 0
              }
            },
            "required": [
              "startTimeMs",
              "stopTimeMs",
              "object"
            ],
            "additionalProperties": false
          }
        }
      }
    }
  ]
}
