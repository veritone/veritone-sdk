{
  "$schema": "https://docs.veritone.com/docs/schemas/vtn-standard/master.json",
  "$id": "https://docs.veritone.com/docs/schemas/vtn-standard/transcript/transcript.json",
  "title": "vtn-standard.transcript",
  "description": "Standard engine output for Transcription at Veritone",
  "type": "object",
  "allOf": [
    {
      "$ref": "../master.json#/definitions/header"
    },
    {
      "properties": {
        "validationContracts": {
          "type": "array",
          "contains": {
            "const": "transcript"
          }
        },
        "series": {
          "type": "array",
          "items": {
            "allOf": [
              {
                "$ref": "../master.json#/definitions/seriesItem"
              },
              {
                "required": ["words"]
              }
            ]
          }
        }
      },
      "required": ["series"]
    }
  ]
}
